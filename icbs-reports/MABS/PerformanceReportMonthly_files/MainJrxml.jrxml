	<queryString language="SQL">
		<![CDATA[select DISTINCT * FROM(
select
'1' as "one", 
(select param_value from institution where id = '1') as "BANK NAME",
A.run_date as "RUNDATE",
A.name || ' - MABS' as "Branch",
B."Account Officer",
C."count" as "No. of Borrowers",
C."sum" as "Loan Account Balance",
sum(B."count") as "# Loans Disbursed",
sum(B."sum") as "Amount of Loans Disbursed",
F."count" as "# New Loans Disbursed",
F."sum" as "Amount New Loans Disbursed",
G."count" as "# Repeat Loans Disbursed",
G."sum" as "Amount Repeat Loans Disbursed",
D."count" as "# of Loans w/Overdue Amount",
case when (C."count" = 0 or C."count" is NULL) OR (D."count" = 0 OR D."count" is NULL) THEN 0 ELSE
round(cast((COALESCE(D."count",0)/COALESCE(C."count",0)::DOUBLE PRECISION)*100 as NUMERIC),2) END as "# of Loans w/Overdue Amount(%)",
E."Balance" as "Portfolio at Risk",
case when (C."sum" = 0 or C."sum" is NULL) or (E."Balance" = 0 or E."Balance" is NULL) THEN 0 ELSE
round(cast((COALESCE(E."Balance",0)/COALESCE(C."sum",0)::DOUBLE PRECISION)*100 as NUMERIC),2)END as "Portfolio at Risk(%)",
(C."Interest"+C."PastDue"+C."Penalty"+C."SC") as "Total Revenue"
from branch A

LEFT JOIN 
(select D.name as "Branch",
C."Account Officer",
count(A.account_no),
sum(A.balance_amount) from loan A
LEFT JOIN (select * from loan_application)B on A.loan_application_id = B.id
LEFT JOIN (select id,name1||' '||name2 as "Account Officer"from user_master)C on B.account_officer:: INTEGER = C.id
LEFT JOIN (select id,name from branch)D on A.branch_id = D.id
LEFT JOIN (select * from product where is_microfinance = true)E on A.product_id = E.id
where E.name is not null and E.name like '%Microfinance MABS%' 
GROUP BY C."Account Officer",D.name ORDER BY D.name,C."Account Officer"
)B on A."name" = B."Branch"

LEFT JOIN (SELECT B.name, D.name1 ||' '|| D.name2 as "Account Officer", count(A.account_no), sum(A.balance_amount),
sum(G.interest_amt) as "Interest",sum(G.past_due_interest_amt) as "PastDue",
sum(G.penalty_amt) as "Penalty",sum(G.service_charge_amt) as "SC" from loan A
LEFT JOIN (SELECT * from branch)B on A.branch_id = B.id
LEFT JOIN (select * from loan_application)C on A.loan_application_id = C."id"
LEFT JOIN (select * from user_master)D on C.account_officer:: INTEGER = D.id
LEFT JOIN (select * from product)F on A.product_id = F.id	
LEFT JOIN (select acct_id,acct_no,branch_id,interest_amt,past_due_interest_amt,
penalty_amt,service_charge_amt from txn_loan_payment_details)G on G.acct_id = A.id
and B.id = G.branch_id where F.is_microfinance = true and F.name like '%Microfinance MABS%' 
group by D.name1, D.name2, B.name order by B.name, D.name1, D.name2)C on A."name" = C."name"
and B."Account Officer" = C."Account Officer"

LEFT JOIN (SELECT B.name,D.name1 ||' '|| D.name2 as "Account Officer",
count(A.account_no) from loan A 
LEFT JOIN (SELECT * from branch)B on A.branch_id = B.id
LEFT JOIN (select * from loan_application)C on A.loan_application_id = C."id"
LEFT JOIN (select * from user_master)D on C.account_officer:: INTEGER = D.id
LEFT JOIN (select * from product)F on A.product_id = F.id	
where F.is_microfinance = true and F.name like '%Microfinance MABS%' 
and A.status_id = 4 group by D.name1,D.name2,B.name order by B.name,D.name2,D.name1)D on A."name" = D."name"
and B."Account Officer" = D."Account Officer"

LEFT JOIN (select B."name",D."Account Officer",sum(A.balance_amount) as "Balance" from loan A
LEFT JOIN (select id,name,run_date from branch)B on A.branch_id = B.id
LEFT JOIN (select * from loan_application)C on A.loan_application_id = C."id"
LEFT JOIN (select id,name1||' '||name2 as "Account Officer" from user_master)D on C.account_officer:: INTEGER = D.id
LEFT JOIN (select * from product)F on A.product_id = F.id	
where F.is_microfinance = true and F.name like '%Microfinance MABS%'
GROUP BY D."Account Officer",B."name")E on A."name" = E."name" and B."Account Officer" = E."Account Officer" 

LEFT JOIN (select Z."Branch",Z."Account Officer",sum(Z."count") as "count",sum(Z."sum") from
(select B.name as "Branch",E."Account Officer",C.display_name,count(A.account_no),sum(A.balance_amount) from loan A
LEFT JOIN (SELECT id,name from branch)B on A.branch_id = B.id
LEFT JOIN (SELECT id,display_name from customer)C on A.customer_id = C."id"
LEFT JOIN (select * from loan_application)D on A.loan_application_id = D.id
LEFT JOIN (select id,name1||' '||name2 as "Account Officer" from user_master)E on D.account_officer:: INTEGER = E.id
LEFT JOIN (select * from product where is_microfinance = true)F on A.product_id = F.id
where A.status_id = 6 and F.name is not null and F.name like '%Microfinance MABS%' 
group by B.name,C.display_name,E."Account Officer" ORDER BY B.name,E."Account Officer"
)Z where Z."count" = 1 group by Z."Branch",Z."Account Officer")F ON A."name" = F."Branch" and B."Account Officer" = F."Account Officer"

LEFT JOIN (select Z."Branch",Z."Account Officer",sum(Z."count") as "count",sum(Z."sum") from
(select B.name as "Branch",E."Account Officer",C.display_name,count(A.account_no),sum(A.balance_amount) from loan A
LEFT JOIN (SELECT id,name from branch)B on A.branch_id = B.id
LEFT JOIN (SELECT id,display_name from customer)C on A.customer_id = C."id"
LEFT JOIN (select * from loan_application)D on A.loan_application_id = D.id
LEFT JOIN (select id,name1||' '||name2 as "Account Officer" from user_master)E on D.account_officer:: INTEGER = E.id
LEFT JOIN (select * from product where is_microfinance = true)F on A.product_id = F.id
where A.status_id = 6 and F.name is not null and F.name like '%Microfinance MABS%' 
group by B.name,C.display_name,E."Account Officer" ORDER BY B.name,E."Account Officer"
)Z where Z."count" > 1 group by Z."Branch",Z."Account Officer")G ON A."name" = G."Branch" and B."Account Officer" = G."Account Officer"
group by A.name,B."Account Officer",C."count",C."sum",D."count",E."Balance",
F."count",F."sum",G."count",G."sum", A.run_date,
C."Interest",C."PastDue",C."Penalty",C."SC" --ORDER BY A.name


UNION ALL


select 
'2' as "one",
(select param_value from institution where caption = 'Institution Name') as "BANK NAME",
A.run_date as "RUNDATE",
A.name || ' - GRAMEEN' as "Branch",
B."Account Officer",
C."count" as "No. of Borrowers",
C."sum" as "Loan Account Balance",
sum(B."count") as "# Loans Disbursed",
