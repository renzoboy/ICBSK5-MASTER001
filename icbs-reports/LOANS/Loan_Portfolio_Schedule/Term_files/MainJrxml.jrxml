	<queryString language="SQL">
		<![CDATA[select 
(select param_value from institution where caption = 'Institution Name') as "BANK NAME",
B.name,
(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where extract(DAY from (A.maturity_date-A.opening_date)) <= 360 
and Z.name = B.name and A.opening_date < $P{PrevMonth}
GROUP BY Z.name) as "Short Term",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where (extract(DAY from (A.maturity_date-A.opening_date)) > 360
and extract(DAY from (A.maturity_date-A.opening_date)) <= 1800) 
and Z.name = B.name and A.opening_date < $P{PrevMonth}
GROUP BY Z.name) as "Medium Term ",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where extract(DAY from (A.maturity_date-A.opening_date)) > 1800 
and Z.name = B.name and A.opening_date < $P{PrevMonth}
GROUP BY Z.name) as "Long Term",

sum(A.balance_amount) as "Total",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where extract(DAY from (A.maturity_date-A.opening_date)) <= 360 
and Z.name = B.name and A.opening_date < (select max(opening_date) from loan where extract(month from opening_date) = ($P{GetDateVar}))
GROUP BY Z.name) as "Prev Short Term",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where (extract(DAY from (A.maturity_date-A.opening_date)) > 360
and extract(DAY from (A.maturity_date-A.opening_date)) <= 1800) 
and Z.name = B.name and A.opening_date < (select max(opening_date) from loan where extract(month from opening_date) =($P{GetDateVar}))
GROUP BY Z.name) as "Prev Medium Term ",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where extract(DAY from (A.maturity_date-A.opening_date)) > 1800 
and Z.name = B.name and A.opening_date < (select max(opening_date) from loan where extract(month from opening_date) =($P{GetDateVar}))
GROUP BY Z.name) as "Prev Long Term",

(select sum(A.balance_amount) as "AllSum" from loan A
LEFT JOIN (select id,name from branch)Z on A.branch_id = Z."id"
where Z.name = B.name and A.opening_date < (select max(opening_date) from loan where extract(month from opening_date) = ($P{GetDateVar}))
GROUP BY Z.name) as "Prev Total",

(select max(opening_date) from loan where extract(month from opening_date) = $P{GetParam}) as "Previous"

from loan A
LEFT JOIN (select id,name from branch)B on A.branch_id = B.id
where A.opening_date < $P{PrevMonth}
GROUP BY B.name
ORDER BY B.name]]>
	</queryString>
