	<queryString language="SQL">
		<![CDATA[select 
	B.param_value as "BANK NAME",
	H.param_value as "BANK ADDRESS",
	C.name as "BRANCH NAME",
	A.account_no as "ACCOUNT NUMBER",
	D.display_name as "ACCOUNT NAME",
	A.pn_no as "P.N. No: SMB",
	A.opening_date as "DATE",
	C.address as "BRANCH ADDRESS",
	A.maturity_date as "DUE DATE",
	A.granted_amount as "AMOUNT",
	A.term as "TERM",
	A.interest_rate as "RATE",
	E.description as "COL DESCRIPTION",
	F.date_of_issuance as "REM DATE",
	G.customer_id as "CO-MAKER"
from loan A
	inner join (select param_value from institution where id = '1')B ON B.param_value is not null
	inner join (select param_value from institution where id = '5')H ON H.param_value is not null
	inner join branch C on A.branch_id = C.ID
	inner join customer D on A.customer_id = D.id
	left join collateral E on A.status_id = E.status_id
	left join collateralrem F on A.customer_id = F.id
	left join loan_application_comaker G on A.id = G.id
	where A.id = $P{Loan_ID}
order by C.name, D.display_name]]>
	</queryString>
