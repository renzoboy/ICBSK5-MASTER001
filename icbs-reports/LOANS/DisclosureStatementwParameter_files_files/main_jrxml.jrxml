	<queryString language="SQL">
		<![CDATA[select
	B.param_value as "BANK NAME",
	C.name as "BRANCH NAME",
	A.account_no as "ACCOUNT NUMBER",
	A.adv_interest as "ADVANCE INTEREST",
	A.interest_rate as "INTEREST RATE",
	N.description as "Kind of Loan",
	A.maturity_date as "MATURITY DATE",
	A.opening_date as "DATE GRANTED",
	A.pn_no as "PN NO.",
	A.term as "TERM OF LOAN",
	A.total_disbursement_amount as "LOAN AMOUNT",
	A.total_net_proceeds as "NET PROCEEDS",
	A.effective_interest_rate / 12 as "EIR",
	D.display_name as "NAME OF BORROWER",
	E.address1 as "BARANGAY",
	E.address2 as "TOWN",
	E.address3 as "CODE",
	case when SUM(K1.amount) is null then 0 else  SUM(K1.amount) end  as "SC Amount",
	case when SUM(K3.amount) is null then 0 else  SUM(K3.amount) end as "DS Amount",
	case when SUM(K4.amount) is null then 0 else  SUM(K4.amount) end as "DSC Amount",
	case when SUM(K2.amount) is null then 0 else  SUM(K2.amount) end as "FF Amount",
	case when SUM(K5.amount) is null then 0 else  SUM(K5.amount) end as "GRT Amount",
	case when SUM(K6.amount) is null then 0 else  SUM(K6.amount) end as "AI Amount",
	case when SUM(K.amount) is null then 0 else  SUM(K.amount) end as "OC Amount"
from loan A
	inner join (select param_value from institution where id = '1') B ON B.param_value is not null
	left join branch C on A.branch_id = C.id
	inner join customer D on A.customer_id = D.id
	left join loan_project N on N.id = A.loan_project_id
	left join address E on D.id = E.customer_id
	left join loan_loan_deduction L on A.id = L.loan_loan_deductions_id
	left join loan_deduction K on L.loan_deduction_id = K.id and K.scheme_id = 1
	left join loan_deduction K1 on L.loan_deduction_id = K1.id and K1.scheme_id = 2	
	left join loan_deduction K2 on L.loan_deduction_id = K2.id and K2.scheme_id = 4
	left join loan_deduction K3 on L.loan_deduction_id = K3.id and K3.scheme_id = 5
	left join loan_deduction K4 on L.loan_deduction_id = K4.id and K4.scheme_id = 6
	left join loan_deduction K5 on L.loan_deduction_id = K5.id and K5.scheme_id = 7
	left join loan_deduction K6 on L.loan_deduction_id = K6.id and K6.scheme_id = 8
where A.id = $P{Loan_ID} and  (K.posted = true or K1.posted = true or K2.posted = true or K3.posted = true
or K4.posted = true or K5.posted = true or K6.posted = true)
group by B.param_value, C.name, A.account_no, A.adv_interest, A.interest_rate, N.description, A.maturity_date,
A.opening_date, A.pn_no, A.term, A.total_disbursement_amount, A.total_net_proceeds, A.effective_interest_rate,
D.display_name, E.address1, E.address2, E.address3]]>
	</queryString>
