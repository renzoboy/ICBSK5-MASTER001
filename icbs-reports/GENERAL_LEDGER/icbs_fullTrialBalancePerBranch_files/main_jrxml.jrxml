	<parameter name="branch_name" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[select 
	branch.id as branch,
	branch.name as branchname,
	ccy.id as ccy,
	ccy.code as ccy_code,
	glsc.parent_id_id ,
	glsc.sort_code ,
	glsc.sort_name,
	
	case when fn_get_glbalance_per_sc_by_branch(glsc.sort_code,'balance',ccy.id,branch.id) >= 0 then abs(fn_get_glbalance_per_sc_by_branch(glsc.sort_code,'balance',ccy.id,branch.id)) else null end as debit,
	case when fn_get_glbalance_per_sc_by_branch(glsc.sort_code,'balance',ccy.id,branch.id) < 0 then abs( fn_get_glbalance_per_sc_by_branch(glsc.sort_code,'balance',ccy.id,branch.id)) else null end as credit ,
	
	(select param_value from institution where id=1) as inst_name,
	(select new_run_date from BRANCH where id=1) as as_of_date
	
from currency as ccy
inner join gl_sort_code as glsc on 1=1
inner join branch as branch on 1=1

where 
	fn_get_glbalance_per_sc_by_branch(glsc.sort_code,'balance',ccy.id,branch.id) <>0
	and branch.name= $P{branch_name}
	
order by branch.id,ccy, glsc.sort_code]]>
	</queryString>
